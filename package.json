{
  "name": "dispatch-construction-logistics",
  "version": "1.0.0",
  "description": "Carrier-First Construction Logistics Platform",
  "main": "index.js",
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "dev": "nodemon src/index.canonical.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev --prefix web\"",
    "build": "tsc",
    "build:web": "cd web && npm run build",
    "start": "node src/index.canonical.js",
    "test:matcher": "node test-matcher.js",
    "test:backend": "echo 'Backend tests pending'",
    "backup": "node tools/backup-and-manifest.js",
    "refscan": "node tools/ref-scan.js",
    "stage:delete": "node tools/stage-delete.js",
    "staging:canonical": "node tools/staging-bringup.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 20",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:web": "cd web && npm run typecheck",
    "deadcode": "ts-prune --project tsconfig.base.json || echo 'Note: Some unused exports are intentional'",
    "depgraph": "madge --circular --extensions ts,tsx,js,jsx src web/src || echo 'Circular dependencies detected'",
    "check:clean": "npm run lint && npm run typecheck && npm run depgraph"
  },
  "keywords": [
    "construction",
    "logistics",
    "freight",
    "dispatch",
    "carrier"
  ],
  "author": "Dispatch SaaS",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.61.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fast-glob": "^3.3.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "redis": "^5.8.3",
    "signature_pad": "^5.1.1",
    "tar-stream": "^3.1.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "madge": "^6.1.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
